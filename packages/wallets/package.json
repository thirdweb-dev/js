{
  "name": "@thirdweb-dev/wallets",
  "version": "0.1.1",
  "main": "dist/thirdweb-dev-wallets.cjs.js",
  "module": "dist/thirdweb-dev-wallets.esm.js",
  "exports": {
    ".": {
      "module": "./dist/thirdweb-dev-wallets.esm.js",
      "default": "./dist/thirdweb-dev-wallets.cjs.js"
    },
    "./evm": {
      "module": "./evm/dist/thirdweb-dev-wallets-evm.esm.js",
      "default": "./evm/dist/thirdweb-dev-wallets-evm.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/thirdweb-dev/js/tree/main/packages/wallets",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thirdweb-dev/js/issues"
  },
  "author": "thirdweb eng <eng@thirdweb.com>",
  "files": [
    "dist/",
    "evm/"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "evm/index.ts"
    ],
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "exports": true
    },
    "exports": true
  },
  "sideEffects": false,
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.226.0",
    "@coinbase/wallet-sdk": "^3.6.0",
    "@magic-ext/connect": "^3.1.0",
    "@magic-ext/oauth": "^4.1.0",
    "@magic-sdk/provider": "^10.1.0",
    "@noble/ed25519": "^1.7.1",
    "@solana/web3.js": "^1.70.1",
    "@wagmi/chains": "^0.1.0",
    "@wagmi/core": "^0.8.2",
    "@walletconnect/ethereum-provider": "^1.7.8",
    "abitype": "^0.2.5",
    "bs58": "^5.0.0",
    "ethers": "^5.7.2",
    "ethers-aws-kms-signer": "^1.3.2",
    "eventemitter3": "^5.0.0",
    "localforage": "^1.10.0",
    "magic-sdk": "^10.1.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.2.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "clean": "rm -rf dist/",
    "build": "tsc && preconstruct build"
  }
}