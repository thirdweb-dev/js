        "./dist/types/exports/utils.d.ts"
      ],
      "wallets": [
        "./dist/types/exports/wallets.d.ts"
      ],
      "wallets/*": [
        "./dist/types/exports/wallets/*.d.ts"
      ],
      "modules": [
        "./dist/types/exports/modules.d.ts"
      ],
      "social": [
        "./dist/types/exports/social.d.ts"
      ],
      "ai": [
        "./dist/types/exports/ai.d.ts"
      ]
    }
