{
  "name": "thirdweb",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thirdweb-dev/js.git#main"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thirdweb-dev/js/issues"
  },
  "author": "thirdweb eng <eng@thirdweb.com>",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./transaction": {
      "types": "./dist/types/transaction/index.d.ts",
      "import": "./dist/esm/transaction/index.js",
      "default": "./dist/cjs/transaction/index.js"
    },
    "./event": {
      "types": "./dist/types/event/index.d.ts",
      "import": "./dist/esm/event/index.js",
      "default": "./dist/cjs/event/index.js"
    },
    "./rpc": {
      "types": "./dist/types/rpc/index.d.ts",
      "import": "./dist/esm/rpc/index.js",
      "default": "./dist/cjs/rpc/index.js"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "default": "./dist/cjs/react/index.js"
    },
    "./storage": {
      "types": "./dist/types/storage/index.d.ts",
      "import": "./dist/esm/storage/index.js",
      "default": "./dist/cjs/storage/index.js"
    },
    "./wallets": {
      "types": "./dist/types/wallets/index.d.ts",
      "import": "./dist/esm/wallets/index.js",
      "default": "./dist/cjs/wallets/index.js"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "default": "./dist/cjs/utils/index.js"
    },
    "./contract": {
      "types": "./dist/types/contract/index.d.ts",
      "import": "./dist/esm/contract/index.js",
      "default": "./dist/cjs/contract/index.js"
    },
    "./wallets/*": {
      "types": "./dist/types/wallets/*.d.ts",
      "import": "./dist/esm/wallets/*.js",
      "default": "./dist/cjs/wallets/*.js"
    },
    "./extensions/*": {
      "types": "./dist/types/extensions/*.d.ts",
      "import": "./dist/esm/extensions/*.js",
      "default": "./dist/cjs/extensions/*.js"
    },
    "./adapters/*": {
      "types": "./dist/types/adapters/*.d.ts",
      "import": "./dist/esm/adapters/*.js",
      "default": "./dist/cjs/adapters/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "react": [
        "./dist/types/react/index.d.ts"
      ],
      "transaction": [
        "./dist/types/transaction/index.d.ts"
      ],
      "event": [
        "./dist/types/event/index.d.ts"
      ],
      "rpc": [
        "./dist/types/rpc/index.d.ts"
      ],
      "storage": [
        "./dist/types/storage/index.d.ts"
      ],
      "wallets": [
        "./dist/types/wallets/index.d.ts"
      ],
      "utils": [
        "./dist/types/utils/index.d.ts"
      ],
      "contract": [
        "./dist/types/contract/index.d.ts"
      ],
      "wallets/*": [
        "./dist/types/wallets/*.d.ts"
      ],
      "extensions/*": [
        "./dist/types/extensions/*.d.ts"
      ],
      "adapters/*": [
        "./dist/types/adapters/*.d.ts"
      ]
    }
  },
  "browser": {
    "crypto": false
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!**/*.tsbuildinfo",
    "!**/*.test.ts",
    "!**/*.test.ts.snap",
    "!**/*.test-d.ts",
    "!**/*.bench.ts",
    "!tsconfig.build.json"
  ],
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@noble/hashes": "1.3.3",
    "@radix-ui/colors": "3.0.0",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-focus-scope": "1.0.4",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-tooltip": "1.0.7",
    "@tanstack/react-query": "5.20.2",
    "@walletconnect/ethereum-provider": "2.11.1",
    "abitype": "1.0.0",
    "bs58": "5.0.0",
    "fuse.js": "7.0.0",
    "mipd": "0.0.5",
    "qrcode": "1.5.3",
    "viem": "2.7.8"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.20.1",
    "@thirdweb-dev/sdk": "workspace:*",
    "@types/bun": "1.0.5",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.2.17",
    "@viem/anvil": "0.0.7",
    "@vitest/coverage-v8": "1.2.2",
    "dotenv-mono": "1.3.13",
    "eslint-config-thirdweb": "workspace:*",
    "eslint-plugin-better-tree-shaking": "0.0.4",
    "eslint-plugin-jsdoc": "48.0.6",
    "eslint-plugin-svg-jsx": "^1.2.2",
    "ethers5": "npm:ethers@5.7.2",
    "ethers6": "npm:ethers@6.11.0",
    "react": "18.2.0",
    "rimraf": "5.0.5",
    "typedoc-gen": "workspace:*",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  },
  "peerDependencies": {
    "ethers": "^5 || ^6",
    "react": ">=18",
    "typescript": ">=5.0.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "ethers": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "bench": "vitest bench",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm && pnpm build:types",
    "build:cjs": "tsc --project ./tsconfig.build.json --module commonjs --outDir ./dist/cjs --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > ./dist/cjs/package.json",
    "build:esm": "tsc --project ./tsconfig.build.json --module es2020 --outDir ./dist/esm && printf '{\"type\": \"module\",\"sideEffects\":false}' > ./dist/esm/package.json",
    "build:types": "tsc --project ./tsconfig.build.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "clean": "rimraf dist",
    "push": "yalc push",
    "test:watch": "vitest -c ./test/vitest.config.ts dev",
    "test": "CI=true vitest -c ./test/vitest.config.ts --coverage --retry=3 --bail=1 --pool=forks",
    "test:cov": "vitest dev -c ./test/vitest.config.ts --coverage",
    "typedoc": "node scripts/typedoc.mjs",
    "update-version": "node scripts/version.mjs"
  },
  "engines": {
    "node": ">=18"
  }
}
