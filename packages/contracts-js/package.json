{
  "name": "@thirdweb-dev/contracts-js",
  "version": "1.1.0",
  "main": "dist/thirdweb-dev-contracts-js.cjs.js",
  "module": "dist/thirdweb-dev-contracts-js.esm.js",
  "license": "Apache-2.0",
  "repository": {
    "directory": "packages/contracts-js",
    "type": "git",
    "url": "https://github.com/thirdweb-dev/js.git"
  },
  "dependencies": {
    "@thirdweb-dev/contracts": "3.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@preconstruct/cli": "^2.2.1",
    "ethers": "^5.7.0",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "ethers": "^5.7.0"
  },
  "files": [
    "abis/",
    "dist/"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "typechain": "rm -rf ./src && typechain --target ethers-v5 --out-dir src '../../node_modules/@thirdweb-dev/contracts/abi/*.json'",
    "extract-abi": "node ./scripts/extract-abis.mjs",
    "build": "yarn typechain && yarn extract-abi && tsc && yarn preconstruct build"
  }
}
