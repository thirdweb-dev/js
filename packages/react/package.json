    "@babel/preset-typescript": "^7.18.6",
    "@microsoft/api-documenter": "^7.19.4",
    "@microsoft/api-extractor": "^7.29.2",
    "@microsoft/tsdoc": "^0.14.1",
    "@playwright/experimental-ct-react": "1.25.1",
    "@playwright/test": "1.25.1",
    "@preconstruct/cli": "^2.2.1",
    "@thirdweb-dev/sdk": "*",
    "@types/color": "^3.0.3",
    "@types/mime": "^3.0.1",
    "@types/react": "^18.0.5",
