    ".": {
      "module": "./dist/thirdweb-dev-auth.esm.js",
      "default": "./dist/thirdweb-dev-auth.cjs.js"
    },
    "./evm": {
      "module": "./evm/dist/thirdweb-dev-auth-evm.esm.js",
      "default": "./evm/dist/thirdweb-dev-auth-evm.cjs.js"
    },
    "./evm/next": {
      "module": "./evm/next/dist/thirdweb-dev-auth-evm-next.esm.js",
      "default": "./evm/next/dist/thirdweb-dev-auth-evm-next.cjs.js"
    },
    "./evm/express": {
      "module": "./evm/express/dist/thirdweb-dev-auth-evm-express.esm.js",
      "default": "./evm/express/dist/thirdweb-dev-auth-evm-express.cjs.js"
    },
    "./solana/next": {
      "module": "./solana/next/dist/thirdweb-dev-auth-solana-next.esm.js",
      "default": "./solana/next/dist/thirdweb-dev-auth-solana-next.cjs.js"
    },
    "./evm/next-auth": {
      "module": "./evm/next-auth/dist/thirdweb-dev-auth-evm-next-auth.esm.js",
      "default": "./evm/next-auth/dist/thirdweb-dev-auth-evm-next-auth.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/thirdweb-dev/js/tree/main/packages/auth",
  "author": "thirdweb eng <eng@thirdweb.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "preconstruct build",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "clean": "rm -rf dist/ && rm -rf node_modules/",
    "test": "mocha --config './test/evm/.mocharc.json' --timeout 30000 --parallel './test/evm/**/*.test.ts'"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "evm/index.ts",
      "evm/express/index.ts",
      "evm/next/index.ts",
      "evm/next-auth/index.ts",
      "solana/next/index.ts"
    ],
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "exports": true
    },
    "exports": true
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
