{
  "name": "@thirdweb-dev/js",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:thirdweb-dev/js.git",
  "author": "thirdweb eng <eng@thirdweb.com>",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.3",
    "eslint": "^8.23.0",
    "eslint-config-acme": "*",
    "prettier": "^2.7.1",
    "turbo": "latest",
    "yarn-deduplicate": "^5.0.2"
  },
  "scripts": {
    "test": "turbo run test",
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "bump-version": "changeset version",
    "release": "turbo run build && changeset publish",
    "postinstall": "yarn-deduplicate yarn.lock",
    "dev-version": "yarn changeset pre enter dev && yarn bump-version"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
