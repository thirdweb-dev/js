{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "sdk#build": {
      "dependsOn": [
        "^build",
        "$factoryAddress",
        "$registryAddress",
        "$contractPublisherAddress"
      ]
    },
    "auth#build": {
      "outputs": ["dist/**", "express/**", "next/**", "next-auth/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*"],
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
