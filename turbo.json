{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["CI", "TW_SECRET_KEY"],
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "bench": {
      "cache": false,
      "dependsOn": []
    },
    "typedoc": {
      "outputs": ["typedoc/**"],
      "dependsOn": ["typedoc-gen#build", "^build"]
    },
    "thirdweb-dashboard#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "portal#build": {
      "outputs": [".next/**", "!.next/cache/**", "searchIndex.json"],
      "dependsOn": [
        "^build",
        "thirdweb#typedoc",
        "@thirdweb-dev/sdk#typedoc",
        "@thirdweb-dev/react#typedoc",
        "@thirdweb-dev/react-core#typedoc",
        "@thirdweb-dev/storage#typedoc",
        "@thirdweb-dev/wallets#typedoc"
      ]
    },
    "portal#dev": {
      "dependsOn": [
        "^build",
        "thirdweb#typedoc",
        "@thirdweb-dev/sdk#typedoc",
        "@thirdweb-dev/react#typedoc",
        "@thirdweb-dev/react-core#typedoc",
        "@thirdweb-dev/storage#typedoc",
        "@thirdweb-dev/wallets#typedoc"
      ]
    },
    "playground-web#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "wallet-ui#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/chains#build": {
      "outputs": ["dist/**", "chains/**"],
      "dependsOn": ["^build"],
      "inputs": ["package.json", "scripts/**", "data/**", "src/**", "chains/**"]
    },
    "@thirdweb-dev/wallets#build": {
      "outputs": ["dist/**", "evm/**"],
      "dependsOn": ["^build"],
      "env": ["THIRDWEB_EWS_SDK_VERSION"]
    },
    "@thirdweb-dev/payments#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": ["THIRDWEB_PAYMENTS_SDK_VERSION"]
    },
    "@thirdweb-dev/pay#build": {
      "outputs": ["dist/**", "integrations/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/sdk#build": {
      "outputs": ["dist/**", "evm/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/react#build": {
      "outputs": ["dist/**", "evm/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/react-core#build": {
      "outputs": ["dist/**", "evm/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/react-native#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/react-native-compat#build": {
      "outputs": ["src/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/auth#build": {
      "outputs": [
        "dist/**",
        "evm/**",
        "express/**",
        "next/**",
        "fastify/**",
        "next-auth/**"
      ],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/contracts-js#build": {
      "outputs": ["dist/**", "factories/**", "src/**", "package.json"],
      "inputs": ["package.json"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/service-utils#build": {
      "outputs": ["dist/**", "cf-worker/**", "node/**"],
      "dependsOn": ["^build"]
    },
    "thirdweb#update-version": {
      "inputs": ["package.json"],
      "outputs": ["src/version.ts"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["^build"]
    },
    "test:legacy": {
      "outputs": ["coverage/**"],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["^build"]
    },
    "e2e": {
      "outputs": [],
      "inputs": ["src/**", "fixtures/**"],
      "dependsOn": ["^build"]
    },
    "e2e:legacy": {
      "outputs": [],
      "inputs": ["src/**", "fixtures/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:legacy": {
      "cache": false,
      "outputs": []
    },
    "fix": {
      "cache": false,
      "outputs": []
    },
    "format": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "thirdweb-dashboard#dev": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
