{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "sdk#build": {
      "dependsOn": ["^build"]
    },
    "auth#build": {
      "outputs": ["dist/**", "express/**", "next/**", "next-auth/**"],
      "dependsOn": ["^build", "$CI"]
    },
    "solana#build": {
      "outputs": ["dist/**", "server/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["build"]
    },
    "e2e": {
      "outputs": [],
      "inputs": ["src/**", "e2e/**"],
      "dependsOn": ["build"]
    },
    "react#e2e": {
      "inputs": ["src/**", "e2e/**", "playwright/**"],
      "dependsOn": ["build", "$CI"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
