{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "sdk#build": {
      "outputs": ["dist/**", "evm/**", "solana/**"],
      "dependsOn": ["^build"]
    },
    "react#build": {
      "outputs": ["dist/**", "evm/**", "solana/**"],
      "dependsOn": ["^build"]
    },
    "auth#build": {
      "outputs": ["dist/**", "express/**", "next/**", "next-auth/**"],
      "dependsOn": ["^build"]
    },
    "contracts-js#build": {
      "outputs": ["dist/**", "src/**"],
      "dependsOn": ["^build"]
    },
    "solana#build": {
      "outputs": ["dist/**", "server/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["^build"]
    },
    "test:solana": {
      "outputs": [],
      "inputs": ["src/solana/**", "test/solana/**"]
    },
    "test:evm": {
      "outputs": [],
      "inputs": ["src/evm/**", "test/evm/**"],
      "dependsOn": ["^build"]
    },
    "e2e": {
      "outputs": [],
      "inputs": ["src/**", "e2e/**"],
      "dependsOn": ["build"]
    },
    "react#e2e": {
      "inputs": ["src/**", "playwright/**"],
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": []
    },
    "fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
